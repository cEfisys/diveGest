<h1>Listing Dive Service Member Gears</h1>

<table>
  <thead>
    <tr>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dive_service_member_gears.each do |dive_service_member_gear| %>
      <tr>
        <td><%= dive_service_member_gear.notes %></td>
        <td><%= link_to 'Show', dive_service_member_gear %></td>
        <td><%= link_to 'Edit', edit_dive_service_member_gear_path(dive_service_member_gear) %></td>
        <td><%= link_to 'Destroy', dive_service_member_gear, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

